{
  "name": "actual-auto-backup",
  "version": "1.0.2",
  "private": true,
  "description": "Automated Actual Budget backups with optional cloud replication.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "backup": "node src/index.js --once",
    "test": "jest --passWithNoTests",
    "test:coverage": "JEST_COVERAGE=true npm test",
    "lint": "eslint --max-warnings=0 .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install || true",
    "prerelease": "npm run lint && npm run format:check && npm test",
    "release": "semantic-release"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@actual-app/api": "^25.11.0",
    "@aws-sdk/client-s3": "^3.673.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "dropbox": "^10.34.0",
    "googleapis": "^144.0.0",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "pino": "^10.1.0",
    "tiny-invariant": "^1.3.3",
    "webdav": "^5.6.0",
    "express": "^4.21.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^9.0.0",
    "@semantic-release/npm": "^13.1.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.3.3",
    "semantic-release": "^25.0.1",
    "supertest": "^6.3.3",
    "tmp": "^0.2.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
